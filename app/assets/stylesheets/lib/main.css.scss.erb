@font-face {
  font-family: 'pinli';
	src:url(font-path('pinli.eot'));
	src:url(font-path('pinli.eot') + '?#iefix') format('embedded-opentype'),
  url(font-path('pinli.svg') + '#pinli') format('svg'),
  url(font-path('pinli.woff')) format('woff'),
  url(font-path('pinli.ttf')) format('truetype');
	font-weight: normal;
	font-style: normal; }

html{
  height:100%;}
  

body {
  padding-top: 50px;
  height:100%;
  background:transparent url(<%= asset_path 'patterns/debut_light_2.png' %>) 0 0 fixed repeat;
  font-size:14px;
  line-height:18px;
  font-family:"Helvetica Neue", Helvetica, sans-serif;
  &.background_cover {
    background-clip:border-box;
    background-color:transparent;
    background-position: bottom left;
    background-repeat:no-repeat;
    background-attachment:fixed;
    background-image:url(<%= asset_path 'backgrounds/1.png' %>);
    .footer_wrapper {
      background:rgba(255, 255, 255, 0.6); } } }

/* Variables */
$themeCOLOR: #76A613;
$themeLinkCOLOR: #2886B4;
$themeCOLORLight: lighten( $themeCOLOR, 36% );
$themeHIGHLIGHT: #505061;

/* Mixins */

@mixin pinli-font {
  font-family:'pinli';
	speak: none;
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased; }

@mixin circular {
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  border-radius: 50%; }

@mixin rounded($radius: 5px) {
  -moz-border-radius: #{$radius} #{$radius} #{$radius} #{$radius};
  -webkit-border-radius: #{$radius} #{$radius} #{$radius} #{$radius};
  border-radius: #{$radius} #{$radius} #{$radius} #{$radius};
}

@mixin boxshadow($a: 0px, $b: 3px, $c: 6px, $color: rgba(0, 0, 0, 0.15)) {
  -webkit-box-shadow: #{$a} #{$b} #{$c} #{$color};
  -moz-box-shadow: #{$a} #{$b} #{$c} #{$color};
  box-shadow: #{$a} #{$b} #{$c} #{$color};
}

@mixin page-header {
  color: #444;
  font-size: 20px;
  line-height: 24px;
  font-weight: normal;
  margin: 0 auto;
  padding: 0 24px 12px;
  text-align:center;
}

/* site wide general */

.right {float:right;}
.left {float:left;}
.clear {clear:both;}
a:focus {outline:none;}

/* typography */

p {
   font-size: 1em; /* 14px */ 
   line-height: 1.2857em; /* 18px */ 
   margin: 0 0 1.2857em 0; }

a {
  color: #1D58AA;
  color: $themeCOLOR;
  &:hover {
    text-decoration: none; } }
    

/* buttons */

.btn {
  display: inline-block;
  text-decoration: none;
  text-align: center;
  position: relative;
  padding: 6px 12px;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.4);
  font-size: 15px;
  line-height:24px;
  -moz-border-radius: 6px;
  -webkit-border-radius: 6px;
  border-radius: 6px;
  background-color:$themeCOLOR;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr = 'transparent', endColorstr = 'rgba(0, 0, 0, 0.15)');
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr = 'transparent', endColorstr = 'rgba(0,0,0,0.15)')";
  background-image: -moz-linear-gradient(top, transparent, rgba(0, 0, 0, 0.15));
  background-image: -ms-linear-gradient(top, transparent, rgba(0, 0, 0, 0.15));
  background-image: -o-linear-gradient(top, transparent, rgba(0, 0, 0, 0.15));
  background-image: -webkit-gradient(linear, center top, center bottom, from(transparent), to(rgba(0, 0, 0, 0.15)));
  background-image: -webkit-linear-gradient(top, transparent, rgba(0, 0, 0, 0.15));
  background-image: linear-gradient(top, transparent, rgba(0, 0, 0, 0.15));
  -moz-background-clip: padding;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  &.action {
    background-color: #2FE72D;
    border: 1px solid #06BE04;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.7);
    padding: 5px 10px 5px 26px;
    position: absolute;
    font-size: 13px; }
  &.follow, &.unfollow {
    top: 5px;
    left: 4px;
    padding:5px 10px;
    overflow: hidden;
    -webkit-transition: all 0.4s ease-in-out;
    -moz-transition: all 0.4s ease-in-out;
    -o-transition: all 0.4s ease-in-out;
    transition: all 0.4s ease-in-out; }
  &.unfollow {
    background-color: #ddd;
    border: 1px solid #bbb;
    &:hover {
      color: #333; } }
  &.follow_in_list {
    background-color: #ddd;
    border: 1px solid #bbb;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.7);
    padding: 5px 10px 5px 10px;
    position: absolute;
    top: 14px;
    right: 0;
    font-size: 13px;
    display: inline-block;
    z-index: 1; }
  &:hover {
    background-image: none; }
  &:active {
    -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3) inset;
    -moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3) inset;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.3) inset; }
  &.primary {
    background-color: $themeCOLOR;
    border: 1px solid $themeCOLOR;
    position: relative;
    font-weight: bold;
    color:#fff; }
  &.sec_action {
    background-color: #F8F8F8;
    border: 1px solid #C6C6C6;
    color: #222;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.7);
    padding: 5px 10px 5px 26px;
    font-size: 13px;
    margin:2px;
    &:hover {
      background-color: #F7F7F7; } }
  &.load_more_button, &.load_more_comments_button {
    padding-left: 10px;
    clear: both;
    margin: 10px auto;
    display: block;
    width: 140px; }
  &.tertiary_action {
    background-color: $themeCOLOR;
    border: 1px solid $themeCOLOR;
    position: relative;
    font-weight: bold; }
  &.btn_facebook {
    background-color: #3c5a97;
    border: 1px solid #3c5a97;
    position: relative;
    font-weight: bold;
    margin: 0 auto;
    color: #fff;
    font-size: 18px;
    display: block;
    width: 240px;
    position: relative;
    &:hover {
      background-color:#658AD9;
      text-decoration:none; }
    &:before {
      content: "\e00e";
    	@include pinli-font;
      position: absolute;
      bottom:-1px;
      left: 6px;
      color: #fff;
      font-size: 28px;
      height: 28px;
      width: 28px; } } }

.loader_icon {
  display: block;
  margin: 0 auto; }


/* Sorting */

.sortable-dragging {
  opacity: 0.6;
}
ul li .handle {
  width: 100%;
  background: #E9E9E9;
  text-align: center;
  cursor: move;
  span {
    display: block;
    padding: 5px;
  }
}
.page-controls {
  float: right;
  margin-top: -30px;
  margin-right: 30px;
  a.stop-sorting {
    display: none;
  }
}

.my-board-controls {
  float: none;
  margin: 0 auto;
  display: block;
  text-align: center;
  color: #ddd;
  margin-bottom: 5px;
  margin-top: -10px;
  a {
    padding: 5px;
    text-decoration:none;
    &:hover {
      color:#000; } } }

/* header */

