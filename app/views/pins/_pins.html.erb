<%= render 'shared/filters' %>

<% if @pins.blank? %>
  <div class="empty-results">
    <h2>No pins to show.</h2>
  </div>
<% else %>
  <ul class="timeline ajax-pagination" id="pins">
    <%= render @pins %>
  </ul>
  <div class="clearfix"></div>
  <%= pagination_link %>
  <div class="clearfix"></div>
<% end %>